<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>PetrolSist.com - @ViewData["Title"] </title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" />
    <environment include="Development">
        <link rel="stylesheet" href="~/css/main.css" asp-append-version="true" />
    </environment>
    <environment include="Staging,Production">
        <link href="~/css/dist/stylesheet.css" rel="stylesheet" asp-append-version="true" />
    </environment>
    <link rel="stylesheet" href="~/PS.Web.Admin.styles.css" asp-append-version="true" />
</head>
<body>

    <div id="app">
        <partial name="_Mobile__Header" />
        <partial name="_Desktop_Header" />

        <div class="main__content">
            @RenderBody()
        </div>

        <partial name="_Footer" />



    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js" integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    
    <environment include="Development">
        <script src="~/js/main.js" asp-append-version="true" type="module"></script>
    </environment>
    <environment include="Staging,Production">
        <script src="~/js/dist/main.js" asp-append-version="true" type="module"></script>
    </environment>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>